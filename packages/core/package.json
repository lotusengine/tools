{
  "name": "@lotusengine/core",
  "version": "1.0.4",
  "description": "LotusEngine core utilities",
  "keywords": [
    "typescript",
    "lotusengine"
  ],
  "homepage": "https://github.com/lotusengine/lotusengine#readme",
  "bugs": "https://github.com/lotusengine/lotusengine/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lotusengine/lotusengine.git"
  },
  "license": "MIT",
  "author": "LotusEngine {https://lotusengine.com/)",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json --force",
    "clean": "tsc --build tsconfig.build.json --clean",
    "coverage": "jest --coverage",
    "depcheck": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "verify": "yarn lint && yarn build && yarn depcheck && yarn coverage",
    "watch": "jest --watch",
    "wipe": "rimraf dist"
  },
  "dependencies": {
    "@lotusengine/schemas": "file: ../schemas",
    "@lotusengine/types": "file: ../types",
    "ajv": "^8.6.3",
    "ajv-formats": "^2.1.1",
    "clean-deep": "^3.4.0",
    "deep-equal": "^2.0.5",
    "fluent-json-schema": "^3.0.1",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/deep-equal": "^1.0.1",
    "@types/lodash": "^4.14.173"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "5fa278879cf7524cb31599c7dff5f02e3f914458"
}
