{
  "name": "@lotusengine/core",
  "version": "1.0.13",
  "description": "LotusEngine core utilities",
  "keywords": [
    "typescript",
    "lotusengine"
  ],
  "homepage": "https://github.com/lotusengine/sdk/blob/master/packages/core/README.md",
  "bugs": "https://github.com/lotusengine/lotusengine/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lotusengine/sdk.git"
  },
  "imports": {
    "#core/*": {
      "default": "./dist/*.js"
    }
  },
  "license": "MIT",
  "author": "LotusEngine {https://lotusengine.com/)",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json --force",
    "clean": "tsc --build tsconfig.build.json --clean",
    "coverage": "jest --coverage",
    "depcheck": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "verify": "yarn lint && yarn build && yarn depcheck && yarn coverage",
    "watch": "jest --watch",
    "wipe": "rimraf dist"
  },
  "dependencies": {
    "@lotusengine/schemas": "^1.0.12",
    "@lotusengine/types": "^1.0.6",
    "ajv": "^8.6.3",
    "ajv-formats": "^2.1.1",
    "clean-deep": "^3.4.0",
    "deep-equal": "^2.0.5",
    "fluent-json-schema": "^3.0.1",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/deep-equal": "^1.0.1",
    "@types/lodash": "^4.14.173"
  },
  "engines": {
    "node": ">=14.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "1f7377e6ca3312f08914d51e97bb53c643f27c2a"
}
